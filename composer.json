{
    "name": "yusam-hub/web-socket",
    "description": "yusam-hub/web-socket",
    "type": "package",
    "minimum-stability": "stable",
    "repositories": {
        "yusam-hub/php-compatibility": {
            "type": "git",
            "url": "https://github.com/yusam-hub/php-compatibility.git"
        }
    },
    "require": {
        "php": "^7.4|^8.0|^8.1|^8.2",

        "ext-curl": "*",
        "ext-json": "*",
        "ext-iconv": "*",
        "ext-mbstring": "*",
        "ext-fileinfo": "*",
        "ext-zmq": "*",
        "ext-pcntl": "*",

        "cboden/ratchet": "^0.4.4",
        "ratchet/pawl": "^0.4.1",
        "react/zmq": "^0.4.0",

        "yusam-hub/php-compatibility": "dev-master"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5"
    },
    "autoload": {
        "psr-4": {
            "YusamHub\\WebSocket\\": "src/"
        },
        "files": [
            "src/functions.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "YusamHub\\WebSocket\\Tests\\": "tests/"
        },
        "files": [
        ]
    }
}
