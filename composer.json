{
    "name": "yusam-hub/web-socket",
    "description": "yusam-hub/web-socket",
    "type": "package",
    "minimum-stability": "stable",
    "require": {
        "php": "^8.3",

        "ext-curl": "*",
        "ext-json": "*",
        "ext-iconv": "*",
        "ext-mbstring": "*",
        "ext-fileinfo": "*",
        "ext-pcntl": "*",

        "cboden/ratchet": "^0.4",
        "ratchet/pawl": "0.3.*",
        "ratchet/rfc6455": "0.3.*",
        "guzzlehttp/psr7": "1.9.*"
    },

    "require-dev": {
        "phpunit/phpunit": "^11.5.3"
    },
    "autoload": {
        "psr-4": {
            "YusamHub\\WebSocket\\": "src/"
        },
        "files": [
            "src/functions.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "YusamHub\\WebSocket\\Tests\\": "tests/"
        },
        "files": []
    }
}

